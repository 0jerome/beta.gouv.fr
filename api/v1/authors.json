---
layout: null
---
[
    {% for author in site.author %}
        {% for last in author.contracts %}
            {% assign contract = last %}
        {% endfor %}
        {
            "id": "{{ author.id }}",
            "fullname": "{{ author.fullname }}",
            "start": "{{ contract.start }}",
            "end": "{{ contract.end }}"
        }
        {% unless forloop.last %},{% endunless %}
    {% endfor %}
]
