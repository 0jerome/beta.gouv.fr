---
layout: null
---
[
    {% for author in site.author %}
        {% for last in author.contracts %}
            {% assign contract = last %}
        {% endfor %}
        {
            "id": "{{ author.id | replace:'/author/','' }}",
            "fullname": "{{ author.fullname }}",
            "start": "{{ contract.start }}",
            "end": "{{ contract.end }}",
            "employer:": "{{ contract.employer }}"
        }
        {% unless forloop.last %},{% endunless %}
    {% endfor %}
]
